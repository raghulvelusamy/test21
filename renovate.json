{
   "$schema":"https://docs.renovatebot.com/renovate-schema.json",
   "extends":[
      "config:recommended",
      ":dependencyDashboard"
   ],
   "enabledManagers":[
      "gradle",
      "maven"
   ],
   "packageRules": [
    {
      "matchManagers": [
        "maven",
        "gradle"
      ],
      "registryUrls": [
        "https://maven.google.com",
        "https://repo.maven.apache.org/maven2"
      ]
    }
  ]
}
